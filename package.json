{
  "name": "scssify",
  "version": "2.2.1",
  "description": "Sass browserify transformer",
  "repository": "cody-greene/scssify",
  "main": "./lib/index.js",
  "browser": "./lib/browser.js",
  "files": ["lib/"],
  "scripts": {
    "watch": "mocha tests/*.js -bwR min",
    "test": "mocha tests/*.js -bR tap"
