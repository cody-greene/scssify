    "browserify-transform",
    "css", "sass", "scss"
  ],
  "author": "Chris Hoage",
  "license": "MIT",
  "engines": {"node": ">=4.0.0"},
  "eslintConfig": {
    "root": true,
    "extends": "./node_modules/@cody-greene/eslint-config/strict.yml",
    "env": {"node": true, "es6": true}
  },
  "devDependencies": {
    "@cody-greene/eslint-config": "3.3.1",
    "autoprefixer": "^6.1.1",
    "jsdom": "^9.8.3",
    "mocha": "^3.1.2",
    "postcss": "^5.1.2"
  },
  "dependencies": {
    "node-sass": "^3.10.1",
    "resolve": "^1.1.6"
  }
